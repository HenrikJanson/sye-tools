{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2015",
        "moduleResolution": "node",
        "inlineSourceMap": true,
        "types": [
            "node"
        ],
        "strict": false,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "lib": [
            "es6",
            "es2016.array.include"
        ]
    },
    "exclude": [
        "node_modules"
    ]
}
