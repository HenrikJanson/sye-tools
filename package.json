{
    "name": "@netinsight/sye-tools",
    "version": "0.9.9",
    "description": "Tools for installing and managing a Sye Backend",
    "main": "index.js",
    "bin": {
        "sye": "cli/sye",
        "sye-aws": "cli/sye-aws",
        "sye-cluster": "cli/sye-cluster",
        "sye-registry": "cli/sye-registry"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/netinsight/sye-tools.git"
    },
    "bugs": {
        "url": "https://github.com/netinsight/sye-tools/issues"
    },
    "scripts": {
        "prepare": "npm run tsc",
        "test": "npm run tsc && multi-tape sye-aws/test/test-*.js",
        "lint": "tslint -t stylish -p tsconfig.json",
        "tsc": "tsc && mv cli/sye.js cli/sye && mv cli/sye-aws.js cli/sye-aws && mv cli/sye-cluster.js cli/sye-cluster && mv cli/sye-registry.js cli/sye-registry && chmod +x cli/sye cli/sye-aws cli/sye-cluster cli/sye-registry"
    },
    "author": "Net Insight",
    "license": "MIT",
    "dependencies": {
        "aws-sdk": "^2.114.0",
        "commander": "^2.11.0",
        "debug": "^3.0.1",
        "easy-table": "^1.1.0",
        "prompt-sync": "^4.1.5",
        "semver": "^5.4.1",
        "source-map-support": "^0.4.18"
    },
    "devDependencies": {
        "@types/commander": "^2.9.2",
        "@types/node": "^8.0.28",
        "multi-tape": "^1.1.1",
        "purple-tape": "^1.0.3",
        "tslint": "^5.7.0",
        "typescript": "^2.5.2"
    }
}
